library(devtools)
devtools::install_github("glmmTMB/glmmTMB") 
install.packages("glmmTMB", repos = "https://glmmTMB.github.io/glmmTMB/repos/")
vignette(package = "glmmTMB")
install.packages("DHARMa")
library(glmmTMB)


data(Salamanders)
head(Salamanders)

cat("Zeros:", round(mean(Salamanders$count == 0)*100,1), "%\n")


m1 <- glmmTMB(count ~ mined + (1|site), family = poisson, data = Salamanders)
m2 <- glmmTMB(count ~ mined + (1|site), ziformula = ~mined, family = poisson, data = Salamanders)
AIC(m1, m2)
library(DHARMa)
sim <- simulateResiduals(m2, n=250)
plot(sim)  
devtools::install_github("glmmTMB/glmmTMB")
